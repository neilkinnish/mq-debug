{
  "name": "MQ Debug",
  "description": "Displays media query events for the current page",
  "version": "1.1",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
      "default_title": "MQ Debug",
      "default_icon": "icon32.png"
  },
  "manifest_version": 2
}